generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model markers {
  id        String   @id @default(uuid())
  latitude  Float
  longitude Float
  pollutionType   String 
  description   String 
  photo  String
}

model circles{
  id        String   @id @default(uuid())
  radius  Float
  color String
  fillColor   String 
  fillOpacity   Float 
}